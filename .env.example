# Multiple Gemini API Keys (one per line)
# The system will automatically rotate between them when one hits quota
GEMINI_API_KEY=your_first_api_key_here
GEMINI_API_KEY_2=your_second_api_key_here
GEMINI_API_KEY_3=your_third_api_key_here

# Other API keys (optional, for future phases)
STABILITY_API_KEY=your_stability_ai_key_here
RUNWAY_API_KEY=your_runway_key_here
ELEVENLABS_API_KEY=your_elevenlabs_key_here
SUNO_API_KEY=your_suno_key_here

# Configuration
GEMINI_REQUESTS_PER_MINUTE=5
